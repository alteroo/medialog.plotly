<?xml version="1.0"?>

<registry>
    
    <records interface="medialog.plotly.interfaces.IPlotlySettings">
        <value key="plotly_username"></value>
        <value key="plotly_api_key"></value>
    </records>
    
    
    <records prefix="plone.resources/medialog-plotly"
        interface='Products.CMFPlone.interfaces.IResourceRegistry'>
        <value key="js">++resource++medialog-plotly/jquery.flextableedit.js</value>
    </records>
    
    <!-- create a bundle definition -->
    <records prefix="plone.bundles/medialog-plotly"
        interface='Products.CMFPlone.interfaces.IBundleRegistry'>
        <value key="resources">
            <element>medialog-plotly</element>
        </value>
        <value key="depends">plone</value>
        <value key="compile">False</value>
        <value key="enabled">True</value>
        <value key="jscompilation">++plone++medialog.plotly/jquery-flextableedit.min.js</value>
        <value key="last_compilation">2015-05-28 00:00:00</value>
    </records>
    
</registry>
